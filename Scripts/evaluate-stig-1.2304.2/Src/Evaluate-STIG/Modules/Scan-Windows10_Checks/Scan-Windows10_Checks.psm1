    $Windows10VersionToEval = "1709"  # this check only valid on Windows 10 prior to 1709
    If ($Windows10Version -ge $Windows10VersionToEval) {
        $Status = "Not_Applicable"
        $FindingDetails += "Windows 10 version is $Windows10Version so this requirement is NA." | Out-String
    }
    Else {
        $TempUserHivePath = ""  # User's loaded hive to perform check
