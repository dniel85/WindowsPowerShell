            $Administrator = $false
            $Account = (Get-LocalUser | Where-Object SID -Like $Item)
            If ($Account.SID -in $AdministratorsMembers.SID) {
                $Administrator = $true
            }
            If ($Account.Enabled -eq $false) {
                $NewObj = [PSCustomObject]@{
                    Name          = $Account.Name
                    SID           = $Account.SID
                    Enabled       = $Account.Enabled
                    Type          = $Account.ObjectClass
                    Administrator = $Administrator
                }
                $CompliantAccounts.Add($NewObj)
            }
            Else {
                $NewObj = [PSCustomObject]@{
                    Name          = $Account.Name
                    SID           = $Account.SID
                    Enabled       = "$($Account.Enabled) [Finding]"
                    Type          = $Account.ObjectClass
                    Administrator = $Administrator
                }
                $NonCompliantAccounts.Add($NewObj)
            }
        }

        ForEach ($Account in $LocalAccounts) {
            $Administrator = $false
            If (($Account.SID -notin $CompliantAccounts.SID) -and ($Account.SID -notin $NonCompliantAccounts.SID)) {
                If ($Account.Name -eq "defaultuser0") {
                    If ($Account.SID -in $AdministratorsMembers.SID) {
